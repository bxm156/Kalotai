<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.assembla.gpsmonsters"
      android:versionCode="1" android:versionName="0.1a" xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
    
    
        <activity android:name=".GPSMonsters"
                  android:label="@string/app_name" android:screenOrientation="portrait" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        


 
<activity android:name=".battle.BattleActivity" android:screenOrientation="landscape" android:launchMode="singleTop"></activity>
<service android:name=".engine.GameManager"></service>
<activity android:screenOrientation="portrait" android:name=".energy.EnergyPoolActivity" android:launchMode="singleTop"></activity>
<activity android:screenOrientation="portrait" android:name=".storyboard.StoryBoardActivity"></activity>
<activity android:screenOrientation="portrait" android:name=".krytselect.KrytSelectActivity"></activity>
<activity android:screenOrientation="portrait" android:name=".krytpedia.KrytpediaMainActivity"></activity>
<activity android:screenOrientation="portrait" android:name=".krytpedia.KrytpediaActivity"></activity>

<activity android:screenOrientation="portrait" android:name=".tokryt.TokrytActivity"></activity>
<uses-library android:name="com.google.android.maps" />
        <activity android:name=".travelpoints.TravelActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
</application>

<uses-permission android:name="android.permission.INTERNET" />    <uses-sdk android:minSdkVersion="7" />
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
<uses-permission android:name="android.permission.VIBRATE"></uses-permission>
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
<uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
</manifest> 